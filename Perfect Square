#perfect square usins Binary Search
def perfSquare(num):
    l,r = 1, num
    
    while l <=r:
        mid = (l+r)//2
        reqVal = mid*mid
        if reqVal == num:
            return mid
        elif reqVal > num:
            r= mid-1
        elif reqVal <num:
            l = mid +1
    return -1
    
    
    return(math.ceil(num)==(math.floor(num)))
    
