# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        current = head
        nextnode = None
        prevnode = None
        
        while current:
            nextnode = current.next
            current.next = prevnode
            prevnode = current
            current = nextnode
            
        return prevnode
