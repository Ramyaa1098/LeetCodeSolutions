class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        l = 0
        result = 0
        for r in range(1,len(prices)):
            if prices[r] > prices[l]:
                result = max(result, prices[r] - prices[l])
            else:
                l = r
        return result
